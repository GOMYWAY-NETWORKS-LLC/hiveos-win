	Identifier     "Layout0"

	#If there is internal GPU this will be uncommened
	#Screen		0  "Screen0" 0 0

	#Nvidias
	Screen		1  "Screen1" 0 0
	Screen		2  "Screen2" 0 0
	Screen      3  "Screen3" 0 0
	Screen      4  "Screen4" 0 0
	Screen      5  "Screen5" 0 0
	Screen      6  "Screen6" 0 0
	Screen      7  "Screen7" 0 0
	Screen      8  "Screen8" 0 0
	Screen      9  "Screen9" 0 0
	Screen      10 "Screen10" 0 0
	Screen      11 "Screen11" 0 0
	Screen      12 "Screen12" 0 0
	Screen      13 "Screen13" 0 0
	Screen      14 "Screen14" 0 0
	Screen      15 "Screen15" 0 0
	Screen      16 "Screen16" 0 0
	Screen      17 "Screen17" 0 0
	Screen      18 "Screen18" 0 0
